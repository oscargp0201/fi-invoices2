{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run app",
            "cwd": "${workspaceFolder}/sapui5",
            "runtimeArgs": [
                "fiori",
                "run"
            ],
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "args": [
                "--open",
                "index.html"
            ],
            "windows": {
                "runtimeExecutable": "npx.cmd"
            },
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/invoices2/sapui5\"}",
                "FIORI_TOOLS_UI5_VERSION": "",
                "FIORI_TOOLS_UI5_URI": "https://ui5.sap.com",
                "FIORI_TOOLS_BACKEND_CONFIG": "[{\"path\":\"/V2\",\"destination\":\"Northwind\"}]"
            }
        },
        {
            "name": "Run Mock Server",
            "cwd": "${workspaceFolder}/sapui5",
            "runtimeArgs": [
                "fiori",
                "run"
            ],
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "args": [
                "--open",
                "test/mockServer.html"
            ],
            "windows": {
                "runtimeExecutable": "npx.cmd"
            },
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/invoices2/sapui5\"}",
                "FIORI_TOOLS_UI5_VERSION": "",
                "FIORI_TOOLS_UI5_URI": "https://ui5.sap.com",
                "FIORI_TOOLS_BACKEND_CONFIG": "[{\"path\":\"/V2\",\"destination\":\"Northwind\"}]"
            }
        }
    ],
    "inputs": [
        {
            "id": "UI5Version",
            "type": "command",
            "command": "sap.ux.appGenerator.ui5Version"
        }
    ]
}